doctype
html
	head
		title= title
		link(rel="stylesheet",href="styles/styles.css")
	body
		#container
			#viewport
				nav#main-menu
					a(href='#/')
						h1
							img(src='./assets/logo.png',width='216',height='96')
					a(href='#/games') Games
					a(href='#/about') About
					a(href='#/blog') Blog
					a(href='#/media') Media
				#page
			#pages
				#home
					include pages/home
				#games
					include pages/games
				#about
					include pages/about
				#blog
					include pages/blog
				#media
					include pages/media

		script(src="scripts/vendor/jquery.min.js")
		script(src="scripts/vendor/jquery.cycle2.min.js")
		script(src="scripts/vendor/three.min.js")
		script(src="scripts/vendor/Mirror.js")
		script(src="scripts/vendor/OrbitControls.js")
		script(src="scripts/vendor/dat.gui.js")
		script(src="scripts/vendor/moment.min.js")
		script(src="socket.io/socket.io.js")
		script(type='x-shader/x-vertex', id='haloVertShader')
			include shaders/halo.vert
		script(type='x-shader/x-vertex', id='haloFragShader')
			include shaders/halo.frag
		script(type='x-shader/x-vertex', id='cloudVertShader')
			include shaders/cloud.vert
		script(type='x-shader/x-vertex', id='cloudFragShader')
			include shaders/cloud.frag
		script(type='x-shader/x-vertex', id='waterVertShader')
			include shaders/water.vert
		script(type='x-shader/x-vertex', id='waterFragShader')
			include shaders/water.frag
		script(src="scripts/L.js")
		script(src="scripts/scenograph.js")
		script(src="scripts/scenograph/objects.js")
		script(src="scripts/scenograph/stats.js")
		script(src="scripts/client.js")
		
