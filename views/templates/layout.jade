script(type="text/x-handlebars")
	nav
		h1
			| {{#link-to 'index'}}
			img(src='/assets/logo.png',alt='Langenium',width='215',height='96')
			| Home
			| {{/link-to}}
		ul#main_menu
			li {{#link-to 'games'}}Games{{/link-to}}
			li {{#link-to 'about'}}About{{/link-to}}
			li {{#link-to 'blog'}}Blog{{/link-to}}
			li {{#link-to 'media'}}Media{{/link-to}}
			li#latency.push-right
				table
					tr
						td Latency
					tr
						td.latency
			li#fps.push-right
				table
					tr
						td FPS
					tr
						td {{fps.now}} ({{fps.min}}-{{fps.max}})
			li#time.push-right
				table
					tr
						td Now
						td Last
						td Delta
					tr
						td {{time.now}}
						td {{time.last}}
						td {{time.delta}}
	h2 
	| {{outlet}}